
<section class="sec">
  
  
  <div  id="logwarn">

  </div>
   
   
 
  <div id="carouselExampleControls" class="carousel slide mt-4" data-ride="carousel">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img
          src="https://images-eu.ssl-images-amazon.com/images/G/31/img19/Wireless/OnePlus/OnePlus6T/PriceDrop/D9867094_IN_WLME_OnePlus_PriceDrop_NewKV_PC_LP_1500x600._CB465812565_.jpg"
          class="d-block " alt="...">
      </div>
      <div class="carousel-item">
        <img src="https://www.tapsmart.com/wp-content/uploads/2017/05/whyswitch.jpg" class="d-block" alt="...">
      </div>
      <div class="carousel-item">
        <img
          src="https://fdn.gsmarena.com/imgroot/news/19/06/xiaomi-mi-9t-malaysia-philippines/-1220x526/gsmarena_003.jpg"
          class="d-block" alt="...">
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev" style="top: 9rem;">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next" style="top: 9rem;">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
 <h3 style="
    position: relative;
    margin-top: 15rem;
    margin-bottom: -13rem;
    margin-left: 7rem;
">Recent Launches</h3>
  {{#each newProducts}}
<div class="card h-100" style="background-color: transparent;width: 18rem;display: inline-block;margin-top: 15rem;margin-left: 6.2rem;">
            <div class="bg-image hover-zoom ripple ripple-surface ripple-surface-light" data-mdb-ripple-color="light">
              <img src="/product-images/{{this._id}}.jpg" class="w-100" style="    height: 18.5rem;;
    border-radius: 30px;" />
              <a href="/product-details/{{this._id}}">
                {{! <div class="mask">
                  <div class="d-flex justify-content-start align-items-end h-100">
                    <h5><span class="badge bg-primary ms-2">New</span></h5>
                  </div>
                </div> }}
                <div class="hover-overlay">
                  <div class="mask" style="background-color: rgba(251, 251, 251, 0.15);"></div>
                </div>
              </a>
            </div>
            <div class="card-body">
              <a href="/product-details?id={{this._id}}" class="text-start text-reset">
                <div class="d-flex justify-content-between">
                  <h5 class="card-title">{{this.productname}}</h5>
             
                </div>
              </a>
              {{!-- <a href="" class="text-start text-reset">
                <p>category</p>
              </a> --}}
             
              <h6 class="text-start"> ₹{{this.price}}</h6>
         

              {{!-- <a href="#" class="btn btn-outline-dark rounded-0 p-1">Add to Wishlist</a> --}}
              {{!-- <a href="/add-to-cart/{{this._id}}"> --}}
               <button class="betaButton" onclick="addToCart('{{this._id}}')" style="
    width: 8rem;
    background: black;
    color: white;
    font-weight: bold;
    font-size: medium;
">Add to cart</button>
            {{#if ../user}}<div class="like-btns"> <button onclick="wishList('{{../user._id}}','{{this._id}}')" class="like float-right "><img
                id="like{{this._id}}" class="icon-like" src="/images/heart.png" alt=""><img id="liked{{this._id}}"
                class="icon-liked" src="/images/liked.png"
                alt=""></button>{{#each ../wishlist }}{{#iff this ../this._id}}{{#if status}}
            <button id="likebtn{{../this.id}}" onclick="unWish('{{../../../user._id}}','{{../../this._id}}')" class="liked float-right liked{{../../this._id}}"><img  class="iconliked" src="/images/liked.png"
                alt=""></button>
            {{/if}}
            {{/iff}}{{/each}}</div>{{/if}}
                </button>
                {{!-- </a> --}}

            </div>
          </div>
{{/each}}
<div class="type m-5">
{{#each categories}}

<h3 style="
    margin-bottom: -13rem;
    margin-left: 3.2rem;
">Buy {{this.category}}</h3>
{{#each ../products}}
     
      {{#ifCond  this.category ../this._id}}
     
    <div class="card h-100 ml-5" style="background-color: transparent;   width: 18rem; display: inline-block;  margin-top: 15rem;">
            <div class="bg-image hover-zoom ripple ripple-surface ripple-surface-light" data-mdb-ripple-color="light">
              <img src="/product-images/{{this._id}}.jpg" class="w-100" style="    height: 18.5rem;;
    border-radius: 30px;" />
              <a href="/product-details/{{this._id}}">
            
                <div class="hover-overlay">
                  <div class="mask" style="background-color: rgba(251, 251, 251, 0.15);"></div>
                </div>
              </a>
            </div>
            <div class="card-body">
              <a href="/product-details?id={{this._id}}" class="text-start text-reset">
                <div class="d-flex justify-content-between">
                  <h5 class="card-title">{{this.productname}} </h5>
             
                </div>
              </a>
              {{!-- <a href="" class="text-start text-reset">
                <p>category</p>
              </a> --}}
             
              <h6 class="text-start"> ₹{{this.price}}</h6>
         

              {{!-- <a href="#" class="btn btn-outline-dark rounded-0 p-1">Add to Wishlist</a> --}}
              {{!-- <a href="/add-to-cart/{{this._id}}"> --}}
               <button class="betaButton" onclick="addToCart('{{this._id}}')" style="
    width: 8rem;
    background: black;
    color: white;
    font-weight: bold;
    font-size: medium;
">Add to cart</button>
            {{#if ../../user}}<div class="like-btns"> <button onclick="wishList('{{../../user._id}}','{{this._id}}')" class="like float-right "><img
                id="like2{{this._id}}" class="icon-like" src="/images/heart.png" alt=""><img id="liked{{this._id}}"
                class="icon-liked" src="/images/liked.png"
                alt=""></button>{{#each ../../wishlist }}{{#iff this ../../this._id}}{{#if status}}
            <button id="likebtn{{../../this.id}}" onclick="unWish('{{../../../../user._id}}','{{../../../this._id}}')" class="liked float-right liked{{../../../this._id}}"><img  class="iconliked" src="/images/liked.png"
                alt=""></button>
            {{/if}}
            {{/iff}}{{/each}}</div>{{/if}}
                </button>
                {{!-- </a> --}}

            </div>
          </div>
 
   
      {{/ifCond}}
{{/each}}
{{/each}}
    <div class="pagination">
          <li class="page-item previous-page disable"><a class="page-link" href="#">Prev</a></li>
          <li class="page-item current-page active"><a class="page-link" href="#">1</a></li>
          <li class="page-item dots"><a class="page-link" href="#">...</a></li>
          <li class="page-item current-page"><a class="page-link" href="#">5</a></li>
          <li class="page-item current-page"><a class="page-link" href="#">6</a></li>
          <li class="page-item dots"><a class="page-link" href="#">...</a></li>
          <li class="page-item current-page"><a class="page-link" href="#">10</a></li>
          <li class="page-item next-page"><a class="page-link" href="#">Next</a></li>
        </div>
</div>




 

</section>
